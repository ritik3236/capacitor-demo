{
  "name": "capacitor-demo",
  "version": "1.0.0",
  "license": "MIT",
  "private": true,
  "homepage": "/",
  "engines": {
    "node": ">=16.20.1 <=20.10.0"
  },
  "scripts": {
    "lint": "eslint --ext .ts,.tsx src",
    "lint:fix": "eslint --ext .ts,.tsx src --fix",
    "mockServer": "node mocks/mockServer.mjs",
    "start": "cross-env webpack-dev-server --config ./webpack/dev.ts --progress",
    "start-mock": "npm-run-all -p start mockServer",
    "build:js": "cross-env NODE_ENV=production webpack --config ./webpack/prod.ts",
    "build": "npm-run-all build:js",
    "clean": "rimraf build",
    "clean:node": "rimraf node_modules",
    "analyze:dev": "cross-env ANALYZE=1 webpack --config ./webpack/dev.ts --progress",
    "analyze:prod": "cross-env ANALYZE=1 webpack --config ./webpack/prod.ts --progress"
  },
  "dependencies": {
    "axios": "^1.6.5",
    "history": "^5.3.0",
    "luxon": "^3.4.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-redux": "^8.1.3",
    "react-router-dom": "^6.21.3",
    "redux": "^4.2.1",
    "redux-saga": "^1.2.3"
  },
  "devDependencies": {
    "@stylistic/eslint-plugin": "^1.5.4",
    "@types/luxon": "^3.4.2",
    "@types/node": "^20.11.5",
    "@types/react": "^18.2.48",
    "@types/react-dom": "^18.2.18",
    "@types/webpack": "^5.28.5",
    "@types/webpack-bundle-analyzer": "^4.6.3",
    "@typescript-eslint/eslint-plugin": "^6.19.1",
    "@typescript-eslint/parser": "^6.19.1",
    "copy-webpack-plugin": "^12.0.2",
    "cross-env": "^7.0.3",
    "css-loader": "^6.9.1",
    "eslint": "^8.56.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "html-webpack-plugin": "^5.6.0",
    "npm-run-all2": "^6.1.1",
    "rimraf": "^5.0.5",
    "style-loader": "^3.3.4",
    "terser-webpack-plugin": "^5.3.10",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3",
    "webpack": "^5.89.0",
    "webpack-bundle-analyzer": "^4.10.1",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1"
  },
  "packageManager": "yarn@4.0.2"
}
